@model WebGrupo3S.Models.TipoServicio

@{
    ViewBag.Title = "Tipos de servicios";
}

<div class="header">
    <h3 class="bg-light-blue-gradient" style="color:white; width:700px; height:40px; text-align:center">Esta seguro que desea dar de baja siguiente registro?</h3>
</div>
<div class="body" style="width:700px">
    <dl class="form-horizontal table-bordered">
        <dt>
            @Html.Label("Nombre")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ts_nombre)
        </dd>

        <dt>
        <dt>
            @Html.Label("Descripción")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ts_descripcion)
        </dd>

        <dt>
            @Html.Label("Cuenta SAP")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ts_CuentaSAP)
        </dd>
        <dt>
            @Html.Label("Coordinación")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ts_Coordinacion)
        </dd>


    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="footer" style="width:700px">
            <button class="btn btn-info btn-xs" type="button" onclick="location.href='@Url.Action("Index", "TServicios")'"><i class="glyphicon glyphicon-remove"></i>No</button>
            <button type="submit" id="myFormSubmit" class="btn btn-success btn-xs"><i class="glyphicon glyphicon-ok"></i> Si</button>
        </div>
    }
</div>
